<div id="registration" class="wrapper">
    <div class="form-group">
        <label for="InputLogin">Login</label>
        <input type="text" class="form-control" id="InputLogin" placeholder="Enter Login">
    </div>
    <div class="form-group">
        <label for="InputPassword">Password</label>
        <input type="text" class="form-control" id="InputPassword" placeholder="Enter Password">
    </div>
    <button class="btn btn-success" id="Check">Submit</button>
    <button class="btn btn-info" id="Register">Register</button>
</div>
